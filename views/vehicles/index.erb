<p><strong><%= "Hi, #{@user}" %></strong></p>
<a href="/vehicles/new">Add a car</a>
<ul id="vehicle_list">
<% @vehicles.each do |v| %>
  <% if v.sale_date == "" %>
    <li class="for_sale"><a href="vehicles/show/<%= v.id %>"><%= "#{v.year} #{v.make} #{v.model}" %></a></li>
  <% else %>
    <li class="sold"><a href="vehicles/show/<%= v.id %>"><%= "#{v.year} #{v.make} #{v.model}" %></a></li>
  <% end %>  
<% end %>
</ul>
